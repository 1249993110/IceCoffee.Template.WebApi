{
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss.fff} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "DbConnectionInfos": {
    "DefaultDbConnectionInfo": {
      "DatabaseType": "SQLite",
      "ConnectionName": "DefaultDbConnection",
      "ConnectionString": "Data Source=./Data/database.db"
    }
  },
  "AccessToken": "123456",
  "EnableSwagger": true,
  "EnableCors": true,
  "AllowedOrigins": null,
  "EnableRequestLog": true,
  "JwtOptions": {
    "ValidIssuer": "https://github.com/1249993110",
    "ValidAudience": "IceCoffee.Template.WebApi",
    "SecretKey": "9jGFvRyWq2wci26714c28a1CI6XmgHo9"
  }
